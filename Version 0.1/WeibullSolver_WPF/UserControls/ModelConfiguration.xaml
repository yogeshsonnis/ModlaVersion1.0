<UserControl x:Class="WeibullSolver_WPF.UserControls.ModelConfiguration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WeibullSolver_WPF.UserControls"
             mc:Ignorable="d">

    <Grid Background="{DynamicResource MaingridBaground }">
        <Grid Margin="30">
            <TabControl Background="Transparent"
                        BorderThickness="0">
                <TabItem Header="Create FM Task">
                    <Grid Margin="0,50,10,0"
                          Height="600"
                          Width="700">
                        <Grid Margin="10"
                              VerticalAlignment="Top"
                              Background="#282828">
                            <Grid Margin="10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                    <RowDefinition Height="42" />
                                </Grid.RowDefinitions>
                                <TextBlock Text="Description :"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <TextBox x:Name="txtDescrition"
                                         Grid.Column="1"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.Description,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                                <TextBlock Text="Disable : "
                                           Grid.Row="1"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <CheckBox Grid.Column="1"
                                          Grid.Row="1"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Center"
                                          Margin="10,0,0,0"
                                          IsChecked="{Binding NewFMTask.Disabled,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                                <TextBlock Text="Secondary : "
                                           Grid.Row="2"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <CheckBox Grid.Column="1"
                                          Grid.Row="2"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Center"
                                          Margin="10,5,0,0"
                                          IsChecked="{Binding NewFMTask.Secondary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock Text="Fixed Interval : "
                                           Grid.Row="3"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <CheckBox Grid.Column="1"
                                          Grid.Row="3"
                                          HorizontalAlignment="Left"
                                          VerticalAlignment="Center"
                                          Margin="10,5,0,0"
                                          IsChecked="{Binding NewFMTask.FixedInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock Text="Planned Tasks : "
                                           Grid.Row="4"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <RadioButton Grid.Column="1"
                                             Grid.Row="4"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Center"
                                             Margin="10,5,0,0"
                                             Style="{StaticResource MyRadioButton}" />
                                <TextBlock Text="Inspection Tasks : "
                                           Grid.Row="5"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <RadioButton x:Name="chkInspectionTask"
                                             Grid.Column="1"
                                             Grid.Row="5"
                                             HorizontalAlignment="Left"
                                             VerticalAlignment="Center"
                                             Margin="10,5,0,0"
                                             Style="{StaticResource MyRadioButton}">
                                </RadioButton>
                                <TextBlock Text="Task Duration : "
                                           Grid.Column="2"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <TextBox x:Name="txtTaskDuration"
                                         Grid.Column="3"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.TaskDuration,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock Text="Task Cost : "
                                           Grid.Column="2"
                                           Grid.Row="1"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <TextBox x:Name="txtTaskCost"
                                         Grid.Column="3"
                                         Grid.Row="1"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.TaskCost,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock Text="Task Interval : "
                                           Grid.Column="2"
                                           Grid.Row="2"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />
                                <TextBox x:Name="txtTaskInterval"
                                         Grid.Row="2"
                                         Grid.Column="3"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.TaskInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                <TextBlock Text="PF Interval : "
                                           Grid.Row="3"
                                           Grid.Column="2"
                                           Width="90"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />

                                <TextBox x:Name="txtPFInterval"
                                         Grid.Row="3"
                                         Grid.Column="3"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.PFInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                         IsEnabled="{Binding ElementName=chkInspectionTask,Path=IsChecked,Mode=TwoWay}" />

                                <TextBlock Text="Detection Probability : "
                                           Grid.Row="4"
                                           Grid.Column="2"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />

                                <TextBox x:Name="txtDetectionProbability"
                                         Grid.Column="5"
                                         Grid.Row="4"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.DetectionProbability,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />
                                <TextBlock Text="AgereductionFactor : "
                                           Width="150"
                                           Grid.Column="2"
                                           Grid.Row="5"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />

                                <TextBox x:Name="txtAgrReductionFactor"
                                         Grid.Column="5"
                                         Grid.Row="5"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.Agereductionfactor,Mode=TwoWay,UpdateSourceTrigger=LostFocus}"
                                         IsEnabled="{Binding ElementName=chkInspectionTask,Path=IsChecked,Mode=TwoWay}" />

                                <TextBlock Text="Effect Cost :"
                                           Width="150"
                                           Grid.Column="2"
                                           Grid.Row="6"
                                           HorizontalAlignment="Left"
                                           FontFamily="{DynamicResource MainFontFamily}"
                                           Foreground="White"
                                           VerticalAlignment="Center"
                                           Margin="5,0,0,0" />

                                <TextBox x:Name="txtEffectCost"
                                         Grid.Column="5"
                                         Grid.Row="6"
                                         Height="25"
                                         Width="150"
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         Text="{Binding NewFMTask.EffectCost,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                                <StackPanel Grid.Row="10"
                                            Grid.ColumnSpan="4"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Margin="0,5,0,0">
                                    <Button Content="Save"
                                            Width="100"
                                            Height="25"
                                            Style="{DynamicResource ButtonStyle}"
                                            Margin="3"
                                            Command="{Binding SaveFMTaskCommand}" />
                                    <Button Content="Clear"
                                            Width="100"
                                            Height="25"
                                            Style="{DynamicResource ButtonStyle}"
                                            Margin="3"
                                            Command="{Binding ClearButtonCommand}"/>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Header="Effects"
                         Width="80">
                    <Grid  Margin="0,50,10,0"
                           Height="600"
                           Width="700">
                        <Grid x:Name="EffectGrid"
                              Margin="10"
                              VerticalAlignment="Top"
                              Background="#282828"
                              Height="250">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="130" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <TextBlock Text="Id : "
                                       Width="110"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox x:Name="txtEffectID"
                                     Grid.Column="1"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewEffect.ID,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Name :"
                                       Grid.Row="1"
                                       Width="90"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox x:Name="txtEffectName"
                                     Grid.Column="1"
                                     Grid.Row="1"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewEffect.Name,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Effect type:"
                                       Grid.Row="2"
                                       Width="100"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox x:Name="txtEffecttype"
                                     Grid.Column="1"
                                     Grid.Row="2"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewEffect.EffectType,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Effects Costs :"
                                       Grid.Row="3"
                                       Width="90"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox x:Name="txtEffectCost1"
                                     Grid.Column="1"
                                     Grid.Row="3"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewEffect.EffectCost,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />
                            <TextBlock Text="Effect Probability :"
                                       Grid.Column="2"
                                       Width="100"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox x:Name="txtEffectProbability"
                                     Grid.Column="3"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewEffect.EffectProbability,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />


                            <TextBlock Text="Apply To Corrective : "
                                       Width="200"
                                       Grid.Row="1"
                                       Grid.Column="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <CheckBox Grid.Column="3"
                                      Grid.Row="1"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      IsChecked="{Binding NewEffect.ApplyToCorrective,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Apply To Planned : "
                                       Width="150"
                                       Grid.Column="2"
                                       Grid.Row="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <CheckBox Grid.Column="3"
                                      Grid.Row="2"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      IsChecked="{Binding NewEffect.ApplyToPlanned,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Apply To Inspection : "
                                       Width="150"
                                       Grid.Column="2"
                                       Grid.Row="3"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <CheckBox Grid.Column="3"
                                      Grid.Row="3"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      IsChecked="{Binding NewEffect.ApplyToInspection,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <StackPanel Grid.Row="4"
                                        Grid.ColumnSpan="2"
                                        Orientation="Horizontal"
                                        HorizontalAlignment="Center"
                                        Margin="0,5,0,0"
                                        Grid.Column="1">
                                <Button Content="Save"
                                        Width="100"
                                        Height="25"
                                        Style="{DynamicResource ButtonStyle}"
                                        Command="{Binding SaveEffectCommand}"
                                        Margin="3" />
                                <Button Content="Clear"
                                        Width="100"
                                        Height="25"
                                        Style="{DynamicResource ButtonStyle}"
                                        Margin="3"
                                        Command="{Binding ClearButtonCommand}"/>
                            </StackPanel>



                        </Grid>

                    </Grid>
                </TabItem>
                <TabItem Header="Create Failuremode"
                         IsSelected="{Binding IsCheckedFailureMode,Mode=TwoWay}">
                    <Grid Margin="0,10,10,0"
                          Background="#282828"
                          Height="600"
                          Width="1000">
                        <Grid x:Name="CreateFailuremode"
                              Margin="10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="120" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <TextBlock Text="ID : "
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox Grid.Column="1"
                                     Height="24"
                                     Width="150"
                                     VerticalAlignment="Bottom"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,13"
                                     Text="{Binding NewFailuremode.ID,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Name :"
                                       Grid.Row="1"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox Grid.Column="1"
                                     Grid.Row="1"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.Name,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Mode Description :"
                                       Grid.Row="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox Grid.Column="1"
                                     Grid.Row="2"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.ModeDescription,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                            <TextBlock Text="Eta :"
                                       Grid.Row="3"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />
                            <TextBox Grid.Column="1"
                                     Grid.Row="3"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.Eta,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />
                            <TextBlock Text="Beta :"
                                       Grid.Row="4"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox Grid.Column="1"
                                     Grid.Row="4"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.Beta,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />
                            <TextBlock Text="Gamma : "
                                       Grid.Row="5"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox Grid.Column="1"
                                     Grid.Row="5"
                                     Height="24"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.Gamma,Mode=TwoWay,UpdateSourceTrigger=LostFocus}" />

                            <TextBlock Text="Initialage :"
                                       Grid.Row="6"
                                       Width="120"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <TextBox Grid.Column="1"
                                     Grid.Row="6"
                                     Height="25"
                                     Width="150"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Margin="5,0,0,0"
                                     Text="{Binding NewFailuremode.Initialage,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />



                            <TextBlock Text="Disabled :"
                                       Width="150"
                                       Grid.Column="0"
                                       Grid.Row="7"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <CheckBox Grid.Column="1"
                                      Grid.Row="7"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0" />

                            <TextBlock Text="Identifiers :"
                                       Width="150"
                                       Grid.Row="8"
                                       Grid.Column="0"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <ComboBox Grid.Column="1"
                                      Grid.Row="8"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0" />

                            <TextBlock Text="Corrective Tasks :"
                                       Width="150"
                                       Grid.Row="9"
                                       Grid.Column="0"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="5,0,0,0" />

                            <ComboBox Grid.Column="1"
                                      Grid.Row="9"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Center"
                                      Margin="5,0,0,0"
                                      DisplayMemberPath="Description"
                                      ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                      SelectedItem="{Binding IsSelectedCorrectiveTask,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                            <TextBlock Text="Planned Tasks :"
                                       Width="150"
                                       Grid.Row="0"
                                       Grid.Column="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="15,0,0,0" />

                            <ListBox Grid.Column="3"
                                     Grid.Row="0"
                                     HorizontalAlignment="Left"
                                     Width="150"
                                     Grid.RowSpan="4"
                                     Margin="5,10,0,20"
                                     Background="Transparent"
                                     ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                     ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <CheckBox HorizontalAlignment="Left"
                                                      VerticalAlignment="Center"
                                                      Margin="5,0,0,0"
                                                      IsChecked="{Binding IsCheckedPlannedTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                                            <TextBlock Grid.Column="1"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Left"
                                                       Margin="5,0,0,0"
                                                       Foreground="White"
                                                       Text="{Binding Description}" />
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>

                            <TextBlock Text="Inspection Tasks :"
                                       Width="150"
                                       Grid.Row="4"
                                       Grid.Column="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Margin="15,0,0,0" />

                            <ListBox Grid.Column="3"
                                     Grid.Row="4"
                                     Grid.RowSpan="4"
                                     HorizontalAlignment="Left"
                                     Width="150"
                                     Margin="5,10,0,20"
                                     Background="Transparent"
                                     ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                     ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <CheckBox HorizontalAlignment="Left"
                                                      VerticalAlignment="Center"
                                                      Margin="5,0,0,0"
                                                      IsChecked="{Binding IsCheckedInspectionTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock Grid.Column="1"
                                                       VerticalAlignment="Center"
                                                       HorizontalAlignment="Left"
                                                       Margin="5,0,0,0"
                                                       Foreground="White"
                                                       Text="{Binding Description}" />
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>

                            <TextBlock Text="Effects :"
                                       Width="150"
                                       Grid.Row="8"
                                       Grid.Column="2"
                                       HorizontalAlignment="Left"
                                       FontFamily="{DynamicResource MainFontFamily}"
                                       Foreground="White"
                                       VerticalAlignment="Center"
