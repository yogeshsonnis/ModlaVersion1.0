<UserControl x:Class="WeibullSolver_WPF.UserControls.ModelConfiguration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WeibullSolver_WPF.UserControls"
             mc:Ignorable="d">
    <Grid x:Name="gridModelConfiguration"
          Background="{DynamicResource MaingridBaground }">
        <Grid  Margin="30">
            <Grid.RowDefinitions>
                <RowDefinition Height="40" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal">
                <RadioButton x:Name="CreateFMTaskTab"
                             Height="25"
                             Width="100"
                             Margin="10,0,0,0"
                             Content="Create FM Task"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
                <RadioButton x:Name="CreateFailuremodeTab"
                             Height="25"
                             Width="115"
                             Margin="10,0,0,0"
                             Content="Create Failuremode"
                             IsChecked="{Binding IsCheckedFailureMode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
                <RadioButton x:Name="CreateFunctionFailureTab"
                             Height="25"
                             Width="135"
                             Content="Create Function Failure"
                             Margin="10,0,0,0"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
                <RadioButton x:Name="CreateFunctionTab"
                             Height="25"
                             Width="100"
                             Margin="10,0,0,0"
                             Content="Create Function"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
                <RadioButton x:Name="CreateLocationtab"
                             Height="25"
                             Width="100"
                             Margin="10,0,0,0"
                             Content="Create Location"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
                <RadioButton x:Name="CreateModelTab"
                             Height="25"
                             Width="100"
                             Margin="10,0,0,0"
                             Content="Create Model"
                             Style="{DynamicResource RadioButtonTabStyle}"
                             Click="CreateFMTaskTab_Click" />
            </StackPanel>
            <Grid Height="600"
                  Width="700"
                  Background="#282828"
                  Grid.Row="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border Background="#353535" />
                <Grid x:Name="CreateFMTAsk"
                      Visibility="Visible"
                      Grid.Row="1"
                      VerticalAlignment="Stretch"
                      Margin="0,10,0,0"
                      HorizontalAlignment="Stretch"
                      Background="{DynamicResource MaingridBaground}">
                    <Grid Height="350"
                          Background="#282828"
                          VerticalAlignment="Top"
                          Margin="0,0,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Text="Description :"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtDescrition"
                                 Grid.Column="1"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.Description,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                        <TextBlock Text="Disable : "
                                   Grid.Row="1"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="1"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,0,0,0"
                                  IsChecked="{Binding NewFMTask.Disabled,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                        <TextBlock Text="Secondary : "
                                   Grid.Row="2"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="2"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0"
                                  IsChecked="{Binding NewFMTask.Secondary,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="Fixed Interval : "
                                   Grid.Row="3"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="3"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0"
                                  IsChecked="{Binding NewFMTask.FixedInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="Planned Tasks : "
                                   Grid.Row="4"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <RadioButton Grid.Column="1"
                                  Grid.Row="4"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0"
                                     Style="{StaticResource MyRadioButton}"/>
                        <TextBlock Text="Inspection Tasks : "
                                   Grid.Row="5"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <RadioButton x:Name="chkInspectionTask"
                                  Grid.Column="1"
                                  Grid.Row="5"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0"
                                  Style="{StaticResource MyRadioButton}">
                        </RadioButton>
                        <TextBlock Text="Task Duration : "
                                   Grid.Column="2"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskDuration"
                                 Grid.Column="3"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.TaskDuration,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="Task Cost : "
                                   Grid.Column="2"
                                   Grid.Row="1"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskCost"
                                 Grid.Column="3"
                                 Grid.Row="1"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.TaskCost,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="Task Interval : "
                                   Grid.Column="2"
                                   Grid.Row="2"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskInterval"
                                 Grid.Row="2"
                                 Grid.Column="3"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.TaskInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="PF Interval : "
                                   Grid.Row="3"
                                   Grid.Column="2"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />

                        <TextBox x:Name="txtPFInterval"
                                 Grid.Row="3"
                                 Grid.Column="3"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.PFInterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                 IsEnabled="{Binding ElementName=chkInspectionTask,Path=IsChecked,Mode=TwoWay}"/>





                        <TextBlock Text="Detection Probability : "
                                   Grid.Row="4"
                                   Grid.Column="2"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />

                        <TextBox x:Name="txtDetectionProbability"
                                 Grid.Column="5"
                                 Grid.Row="4"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.DetectionProbability,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        <TextBlock Text="AgereductionFactor : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="5"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtAgrReductionFactor"
                                 Grid.Column="5"
                                 Grid.Row="5"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.Agereductionfactor,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                 IsEnabled="{Binding ElementName=chkInspectionTask,Path=IsChecked,Mode=TwoWay}" />
                        <TextBlock Text="Effect Cost :"
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="6"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtEffectCost"
                                 Grid.Column="5"
                                 Grid.Row="6"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0"
                                 Text="{Binding NewFMTask.EffectCost,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                        
                        
                        
                        <StackPanel Grid.Row="10"
                                    Grid.ColumnSpan="4"
                                    Orientation="Horizontal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Margin="0,5,0,0">
                            <Button Content="Save"
                                    Width="100"
                                    Height="25"
                                    Style="{DynamicResource ButtonStyle}"
                                    Margin="3"
                                    Command="{Binding SaveFMTaskCommand}" />
                            <Button Content="Close"
                                    Width="100"
                                    Height="25"
                                    Style="{DynamicResource ButtonStyle}"
                                    Margin="3" />
                        </StackPanel>



                    </Grid>
                </Grid>
                <Grid x:Name="CreateFailuremode"
                      Margin="10"
                      Grid.Row="1"
                      Visibility="Collapsed">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <TextBlock Text="ID : "
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <TextBox Grid.Column="1"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,7,0,12"
                             Text="{Binding NewFailuremode.ID,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Name :"
                               Grid.Row="1"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="1"
                             Grid.Row="1"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Component,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Mode Description :"
                               Grid.Row="2"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="1"
                             Grid.Row="2"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.ModeDescription,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Eta :"
                               Grid.Row="3"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="1"
                             Grid.Row="3"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Eta,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Beta :"
                               Grid.Row="4"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <TextBox Grid.Column="1"
                             Grid.Row="4"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Beta,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Gamma : "
                               Grid.Row="5"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <TextBox Grid.Column="1"
                             Grid.Row="5"
                             Height="24"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Gamma,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                    <TextBlock Text="Intialize :"
                               Grid.Row="6"
                               Width="120"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="1"
                             Grid.Row="6"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Initialage,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                    <TextBlock Text="Solve :"
                               Width="150"
                               Grid.Column="0"
                               Grid.Row="11"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <CheckBox Grid.Column="1"
                              Grid.Row="11"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0"
                              IsChecked="{Binding IsCheckedSolve,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                              Command="{Binding CheckSolve}" />

                    <TextBlock Text="Corrective Costs Total :"
                               Grid.Column="2"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="3"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.CorrectiveCostsTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Planned Costs Total :"
                               Grid.Column="2"
                               Grid.Row="1"
                               Width="120"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Top"
                               Margin="5,20,0,0" />
                    <TextBox Grid.Column="3"
                             Grid.Row="1"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Top"
                             HorizontalAlignment="Left"
                             Margin="5,10,0,0"
                             Text="{Binding NewFailuremode.PlannedCostsTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Inspection Costs Total :"
                               Grid.Row="2"
                               Grid.Column="2"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,10,0,0" />
                    <TextBox Grid.Column="3"
                             Grid.Row="2"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,10,0,0"
                             Grid.ColumnSpan="2"
                             Text="{Binding NewFailuremode.InspectionCostsTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Effects Costs Total :"
                               Grid.Row="3"
                               Grid.Column="2"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               />
                    <TextBox 
                             Grid.Row="3"
                             Grid.Column="3"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.EffectsCostsTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                    <TextBlock Text="Failure Profile Total : "
                               Grid.Row="4"
                               Grid.Column="2"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox x:Name="txtFailueProfileTotal1"
                             Grid.Row="4"
                             Grid.Column="4"
                             Margin="5,0,0,0"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Text="{Binding NewFailuremode.FailureProfileTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Costs Total : "
                               Grid.Column="2"
                               Grid.Row="5"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <TextBox x:Name="txtCostsTotal1"
                             Grid.Column="3"
                             Grid.Row="5"
                             Height="24"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.CostsTotal,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                    <TextBlock Text="Time Window : "
                               Grid.Column="2"
                               Grid.Row="6"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <TextBox x:Name="txtFuctionName1"
                             Grid.Column="3"
                             Grid.Row="6"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.TimeWindows,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Project Interval : "
                               Grid.Column="2"
                               Grid.Row="7"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox x:Name="txtFuctionDescription1"
                             Grid.Column="3"
                             Grid.Row="7"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Projectinterval,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Project Life :"
                               Grid.Column="2"
                               Grid.Row="8"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <TextBox Grid.Column="3"
                             Grid.Row="8"
                             Height="25"
                             Width="150"
                             VerticalAlignment="Center"
                             HorizontalAlignment="Left"
                             Margin="5,0,0,0"
                             Text="{Binding NewFailuremode.Projectlife,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                    <TextBlock Text="Apply Inspection At Time Zero :"
                               Grid.Column="2"
                               Grid.Row="9"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />
                    <CheckBox Grid.Column="3"
                              Grid.Row="9"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0" />
                    <TextBlock Text="Disabled :"
                               Width="150"
                               Grid.Column="2"
                               Grid.Row="10"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <CheckBox Grid.Column="3"
                              Grid.Row="10"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0" />
                    <TextBlock Text="Identifiers :"
                               Width="150"
                               Grid.Row="10"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <ComboBox Grid.Column="1"
                              Grid.Row="10"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0" />
                    <TextBlock Text="Corrective Tasks :"
                               Width="150"
                               Grid.Row="7"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <ComboBox Grid.Column="1"
                              Grid.Row="7"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0"
                              DisplayMemberPath="Description"
                              ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                              SelectedItem="{Binding IsSelectedCorrectiveTask,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>

                    <TextBlock Text="Planned Tasks :"
                               Width="150"
                               Grid.Row="8"
                               HorizontalAlignment="Left"
                               FontFamily="{DynamicResource MainFontFamily}"
                               Foreground="White"
                               VerticalAlignment="Center"
                               Margin="5,0,0,0" />

                    <!--<ComboBox Grid.Column="3"
                              Grid.Row="9"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="5,0,0,0"
                              DisplayMemberPath="Description"
                              ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />-->

                    <ListBox Grid.Column="1"
                             Grid.Row="8"
                             HorizontalAlignment="Left"
                             Width="150"
                             Margin="5,0,0,5"
                             Background="Transparent"
                             ItemsSource="{Binding FMTasks,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                             ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox HorizontalAlignment="Left"
                                              VerticalAlignment="Center"
                                              Margin="5,0,0,0"
