<CustomControl:CustomWindow x:Class="WeibullSolver_WPF.MainWindow"
                            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                            xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                            xmlns:local="clr-namespace:WeibullSolver_WPF"
                            xmlns:CustomControl="clr-namespace:WeibullSolver_WPF.CommonControls"
                            xmlns:control="clr-namespace:WeibullSolver_WPF.UserControls"
                            mc:Ignorable="d"
                            Title="MainWindow"
                            WindowState="Maximized">
    <Grid x:Name="mainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="35" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid  Background="{DynamicResource GridBackColor}"
               Grid.ColumnSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <Image Source="/WeibullSolver_WPF;component/Images/Logo.png"
                   Margin="7,0,0,0"
                   Height="25"
                   Width="100"
                   HorizontalAlignment="Left" />

            <StackPanel Grid.Column="1"
                        Orientation="Horizontal">
                <RadioButton x:Name="btnHome"
                             Content="Home"
                             Height="20"
                             Width="50"
                             Click="btnOverview_Click" />
                <RadioButton x:Name="btnOverview"
                             Content="Overview"
                             Height="20"
                             Width="70"
                             Click="btnOverview_Click" />
                <RadioButton x:Name="btnModelSettings"
                             Content="Model Settings"
                             Click="btnOverview_Click"
                             Height="20"
                             Width="100" />
                <RadioButton x:Name="btnModelConfiguration"
                             Content="Model Configuration"
                             Height="20"
                             Width="125"
                             Click="btnOverview_Click" />
                <RadioButton x:Name="btnBatching"
                             Content="Batching"
                             Height="20"
                             Width="60"
                             Click="btnOverview_Click" />
                <RadioButton x:Name="btnHelp"
                             Content="Help"
                             Height="20"
                             Width="45"
                             Click="btnOverview_Click" />
            </StackPanel>

        </Grid>
        <Border x:Name="borderFilters"
                Grid.Row="1"
                Height="30"
                BorderThickness="0,1,0,1"
                BorderBrush="Black">

            <StackPanel Orientation="Horizontal"
                        Background="{DynamicResource GridBackColor}">
                <StackPanel Width="5"
                            Orientation="Vertical"
                            VerticalAlignment="Center"
                            Margin="5,0,0,0">
                    <Border Height="2"
                            Margin="0,2,0,0"
                            Background="{DynamicResource SecondaryDarkGrayColor1}" />
                    <Border Height="2"
                            Margin="0,2,0,0"
                            Background="{DynamicResource SecondaryDarkGrayColor1}" />
                    <Border Height="2"
                            Margin="0,2,0,0"
                            Background="{DynamicResource SecondaryDarkGrayColor1}" />
                    <Border Height="2"
                            Margin="0,2,0,0"
                            Background="{DynamicResource SecondaryDarkGrayColor1}" />
                    <Border Height="2"
                            Margin="0,2,0,0"
                            Background="{DynamicResource SecondaryDarkGrayColor1}" />

                </StackPanel>

                <TextBlock Text="Filters"
                           Foreground="{DynamicResource WhiteColor}"
                           FontFamily="{DynamicResource MainFontFamily}"
                           FontWeight="Bold"
                           VerticalAlignment="Center"
                           Margin="10,0,0,0" />
                <Border Width="1.5"
                        Background="{DynamicResource BorderBackColor}"
                        Margin="10,3,0,2" />
                <RadioButton x:Name="checkboxAll"
                             Content="All"
                             Margin="10,0,0,0"
                             Style="{StaticResource MyRadioButton}" />
                <RadioButton x:Name="checkboxAssetinfo"
                             Content="Assets Information"
                             Margin="15,0,0,0"
                             Style="{StaticResource MyRadioButton}" />
                <RadioButton x:Name="checkboxDecisons"
                             Content="Decisions"
                             Margin="15,0,0,0"
                             Style="{StaticResource MyRadioButton}" />
                <RadioButton x:Name="checkboxModes"
                             Content="Modes"
                             Margin="15,0,0,0"
                             Style="{StaticResource MyRadioButton}" />
                <RadioButton x:Name="checkboxPrimaryTask"
                             Content="Primary Tasks"
                             Margin="15,0,0,0"
                             Style="{StaticResource MyRadioButton}" />
                <RadioButton  x:Name="checkboxResult"
                              Content="Results"
                              Margin="15,0,0,0"
                              Style="{StaticResource MyRadioButton}" />
            </StackPanel>
        </Border>
        <Grid x:Name="gridOverview"
              Grid.Row="3"
              Background="{DynamicResource MaingridBaground }"
              Visibility="Visible">
            <Grid.RowDefinitions>
                <RowDefinition x:Name="rowALL">
                    <RowDefinition.Style>
                        <Style TargetType="RowDefinition">
                            <Setter Property="Height"
                                    Value="0" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=checkboxAssetinfo,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </RowDefinition.Style>
                </RowDefinition>
                <RowDefinition x:Name="rowDecisions">
                    <RowDefinition.Style>
                        <Style TargetType="RowDefinition">
                            <Setter Property="Height"
                                    Value="0" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=checkboxDecisons,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.8*" />
                                </DataTrigger>

                                <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </RowDefinition.Style>
                </RowDefinition>
                <RowDefinition x:Name="rowModes">
                    <RowDefinition.Style>
                        <Style TargetType="RowDefinition">
                            <Setter Property="Height"
                                    Value="0" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=checkboxModes,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=checkboxPrimaryTask,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="0.9*" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </RowDefinition.Style>
                </RowDefinition>
                <RowDefinition x:Name="rowResult">
                    <RowDefinition.Style>
                        <Style TargetType="RowDefinition">
                            <Setter Property="Height"
                                    Value="0" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=checkboxResult,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="1.1*" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                             Value="True">
                                    <Setter Property="Height"
                                            Value="1.1*" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </RowDefinition.Style>
                </RowDefinition>
            </Grid.RowDefinitions>
            <Grid x:Name="gridAssetInfo">
                <control:AssetsInformation Margin="10" />
            </Grid>

            <Grid x:Name="gridDecisions"
                  Margin="15"
                  Grid.Row="1">
                <control:Decisions />
            </Grid>
            <Grid x:Name="gridModes"
                  Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition x:Name="ColumnMode">
                        <ColumnDefinition.Style>
                            <Style TargetType="ColumnDefinition">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=checkboxModes,Path=IsChecked}"
                                                 Value="True">
                                        <Setter Property="Width"
                                                Value="*" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=checkboxModes,Path=IsChecked}"
                                                 Value="False">
                                        <Setter Property="Width"
                                                Value="0" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                                 Value="True">
                                        <Setter Property="Width"
                                                Value="*" />
                                    </DataTrigger>

                                </Style.Triggers>
                            </Style>
                        </ColumnDefinition.Style>
                    </ColumnDefinition>
                    <ColumnDefinition x:Name="ColumnPrimaryTask">
                        <ColumnDefinition.Style>
                            <Style TargetType="ColumnDefinition">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=checkboxPrimaryTask,Path=IsChecked}"
                                                 Value="True">
                                        <Setter Property="Width"
                                                Value="*" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=checkboxPrimaryTask,Path=IsChecked}"
                                                 Value="False">
                                        <Setter Property="Width"
                                                Value="0" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=checkboxAll,Path=IsChecked}"
                                                 Value="True">
                                        <Setter Property="Width"
                                                Value="*" />
                                    </DataTrigger>

                                </Style.Triggers>
                            </Style>
                        </ColumnDefinition.Style>
                    </ColumnDefinition>
                </Grid.ColumnDefinitions>
                <control:Modes x:Name="gridmodes1"
                               Margin="10" />
                <control:PrimaryTasks x:Name="gridmodes2"
                                      Grid.Column="1"
                                      Margin="10" />
            </Grid>
            <Grid x:Name="gridResult"
                  Grid.Row="3">
                <control:Results Margin="10" />
            </Grid>
        </Grid>
        <Grid x:Name="gridModelsettings"
              Grid.Row="3"
              Background="{DynamicResource MaingridBaground }"
              Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <control:ModelSettings x:Name="mdSetting" Margin="10" />
        </Grid>
        <Grid x:Name="gridHome"
              Visibility="Visible"
              Grid.Row="3"
              Background="{DynamicResource MaingridBaground }">
            <StackPanel Orientation="Horizontal"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Left"
                        Margin="50,30,0,0">
                <Button Content="New Model"
                        Width="75"
                        Height="25"
                        Style="{DynamicResource ButtonStyle}"
                        Command="{Binding AddNewModelCommand}" />
                <Button Content="Save Model"
                        Width="75"
                        Height="25"
                        Style="{DynamicResource ButtonStyle}"
                        Command="{Binding SaveModelCommand}"
                        Margin="5,0,0,0" />
                <Button Content="Load Model"
                        Height="25"
                        Width="75"
                        Margin="5,0,0,0"
                        Command="{Binding LoadModelCommand}"
                        Style="{DynamicResource ButtonStyle}" />
            </StackPanel>
            <!--<TextBlock Text="Home"
                       Foreground="White"
                       VerticalAlignment="Top"
                       HorizontalAlignment="Center"
                       FontSize="20"
                       Margin="0,150,0,0"/>-->

        </Grid>
        <Grid x:Name="gridModelConfiguration"
              Visibility="Collapsed"
              Grid.Row="3"
              Background="{DynamicResource MaingridBaground }">
            <Grid  Margin="30">
                <Grid.RowDefinitions>
                    <RowDefinition Height="40" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal">
                    <RadioButton x:Name="CreateFMTaskTab"
                            Height="25"
                            Width="100"
                            Margin="10,0,0,0"
                            Content="Create FM Task"
                            Style="{DynamicResource RadioButtonTabStyle}"
                            Click="CreateFMTaskTab_Click"/>
                    <RadioButton x:Name="CreateFailuremodeTab"
                                 Height="25"
                                 Width="115"
                                 Margin="10,0,0,0"
                                 Content="Create Failuremode"
                                 Style="{DynamicResource RadioButtonTabStyle}"
                                 Click="CreateFMTaskTab_Click"/>
                    <RadioButton x:Name="CreateFunctionFailureTab" Height="25"
                            Width="135"
                            Content="Create Function Failure"
                            Margin="10,0,0,0"
                            Style="{DynamicResource RadioButtonTabStyle}"
                            Click="CreateFMTaskTab_Click"/>
                    <RadioButton x:Name="CreateFunctionTab" Height="25"
                            Width="100"
                            Margin="10,0,0,0"
                            Content="Create Function"
                            Style="{DynamicResource RadioButtonTabStyle}"
                            Click="CreateFMTaskTab_Click"/>
                    <RadioButton x:Name="CreateLocationtab" Height="25"
                            Width="100"
                            Margin="10,0,0,0" 
                            Content="Create Location"
                            Style="{DynamicResource RadioButtonTabStyle}"
                            Click="CreateFMTaskTab_Click"/>
                    <RadioButton x:Name="CreateModelTab" Height="25"
                                 Width="100"
                                 Margin="10,0,0,0"
                                 Content="Create Model"
                                 Style="{DynamicResource RadioButtonTabStyle}"
                                 Click="CreateFMTaskTab_Click"/>
                </StackPanel>
                <Grid Height="550"
                      Width="700"
                      Background="#282828"
                      Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border Background="#353535" />
                    <Grid x:Name="CreateFMTAsk"
                          Grid.Row="1"
                          Margin="10"
                          Visibility="Visible">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Text="Description :"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtDescrition"
                                 Grid.Column="1"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="Disable : "
                                   Grid.Row="1"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="1"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,0,0,0" />

                        <TextBlock Text="Secondary : "
                                   Grid.Row="2"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="2"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0" />
                        <TextBlock Text="Fixed Interval : "
                                   Grid.Row="3"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <CheckBox Grid.Column="1"
                                  Grid.Row="3"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="10,5,0,0" />
                        <TextBlock Text="Task Interval : "
                                   Grid.Row="4"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskDuration"
                                 Grid.Column="1"
                                 Grid.Row="4"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="Task Cost : "
                                   Grid.Row="5"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskCost"
                                 Grid.Column="1"
                                 Grid.Row="5"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />

                        <TextBlock Text="Task Interval : "
                                   Grid.Row="6"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtTaskInterval"
                                 Grid.Column="1"
                                 Grid.Row="6"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="PF Interval : "
                                   Grid.Row="7"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />

                        <TextBox x:Name="txtPFInterval"
                                 Grid.Column="1"
                                 Grid.Row="7"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="Occurances : "
                                   Grid.Row="8"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="1"
                                  Grid.Row="8"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Costs : "
                                   Grid.Row="9"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="1"
                                  Grid.Row="9"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Detection Probability : "
                                   Grid.Column="2"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />

                        <TextBox x:Name="txtDetectionProbability"
                                 Grid.Column="5"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="AgereductionFactor : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="1"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtAgrReductionFactor"
                                 Grid.Column="5"
                                 Grid.Row="1"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="Effect Cost :"
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="2"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <TextBox x:Name="txtEffectCost"
                                 Grid.Column="5"
                                 Grid.Row="2"
                                 Height="25"
                                 Width="150"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left"
                                 Margin="10,0,0,0" />
                        <TextBlock Text="Task PDF : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="3"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="3"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Task HF : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="4"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="4"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Adjusted Task HF : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="5"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="5"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="HF Planned Costs : "
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="6"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="6"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Adjusted HF Planned Costs :"
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="7"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="7"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="PDF Planned Costs :"
                                   Width="150"
                                   Grid.Column="2"
                                   Grid.Row="8"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="5"
                                  Grid.Row="8"
                                  Margin="10,0,0,0"
                                  HorizontalAlignment="Left" />

                        <StackPanel Grid.Row="10"
                                    Grid.ColumnSpan="4"
                                    Orientation="Horizontal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Margin="0,5,0,0">
                            <Button Content="Save"
                                    Width="100"
                                    Height="25"
                                    Style="{DynamicResource ButtonStyle}"
                                    Margin="3" />
                            <Button Content="Close"
                                    Width="100"
                                    Height="25"
                                    Style="{DynamicResource ButtonStyle}"
                                    Margin="3" />
                        </StackPanel>



                    </Grid>
                    <Grid x:Name="CreateFailuremode"
                          Margin="10"
                          Grid.Row="1"
                          Visibility="Collapsed">
                        <TextBlock Text="Create Failuremode"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"/>
                    </Grid>
                    <Grid x:Name="CreateFunctionFailure"
                          Margin="10"
                          Grid.Row="1"
                          Visibility="Collapsed">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="130" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="130" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <TextBlock Text="Corrrective Costs : "
                                   Width="110"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />

                        <ComboBox Grid.Column="1"
                                  Margin="5,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Planned Costs :"
                                   Grid.Row="1"
                                   Width="90"
                                   HorizontalAlignment="Left"
                                   FontFamily="{DynamicResource MainFontFamily}"
                                   Foreground="White"
                                   VerticalAlignment="Center"
                                   Margin="5,0,0,0" />
                        <ComboBox Grid.Column="1"
                                  Grid.Row="1"
                                  Margin="5,0,0,0"
                                  HorizontalAlignment="Left" />
                        <TextBlock Text="Inspection Costs :"
                                   Grid.Row="2"
                                   Width="100"
